{
  "name": "@queekai/client-sdk",
  "version": "0.1.1",
  "private": false,
  "description": "Official Queek storefront client SDK for auth and API requests.",
  "license": "UNLICENSED",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "lint": "tsc --noEmit -p tsconfig.json",
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "test": "node --test tests/*.test.mjs",
    "prepublishOnly": "npm run lint && npm run build && npm test"
  },
  "devDependencies": {
    "typescript": "^5.9.2"
  }
}
